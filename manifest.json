{
    "name" : "SupportSearch",
    "description" : "A Clubspeed support assisting tool!",
    "version" : "1.2",
    "manifest_version" : 3,
    "permissions" : ["storage", "activeTab", "scripting"],
    "host_permissions" : [
        "https://*.clubspeedtiming.com/*",
        "<all_urls>"
    ],
    "action" : {
        "default_popup" : "popup.html",
        "default_icon": {
            "16": "/images/logo_16.png",
            "32": "/images/logo_32.png",
            "48": "/images/logo_48.png",
            "128": "/images/logo_128.png"
          }
    },
    "icons": {
        "16": "/images/logo_16.png",
        "32": "/images/logo_32.png",
        "48": "/images/logo_48.png",
        "128": "/images/logo_128.png"
    }
}